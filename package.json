{
  "name": "beat-link-trigger-overlay",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "parcel build public/overlay.html --dist-dir dist --public-url public/",
    "start": "parcel public/overlay.html --dist-dir dist --public-url public/",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . \"!test*\" --check",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "run-s generate:api fix",
    "generate:api": "openapi-generator-cli generate"
  },
  "dependencies": {
    "@xstate/react": "^3.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.1",
    "styled-components": "^5.3.5",
    "xstate": "^4.32.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@mdx-js/react": "^2.1.1",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-docs": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/react": "^6.5.8",
    "@types/babel__core": "^7.1.19",
    "@types/mdx-js__react": "^1.5.5",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.6.0",
    "prettier": "^2.6.2",
    "react-is": "^18.1.0",
    "typescript": "^4.7.3"
  },
  "browserslist": [
    "last 2 Firefox versions",
    "last 2 Chrome versions"
  ],
  "@parcel/bundler-default": {
    "http": 1
  }
}
