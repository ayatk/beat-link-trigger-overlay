{
  "name": "beat-link-trigger-overlay",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.0.2",
  "scripts": {
    "build": "parcel build public/overlay.html --dist-dir dist --public-url public/",
    "start": "parcel public/overlay.html --dist-dir dist --public-url public/",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . \"!test*\" --check",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "run-s generate:api fix",
    "generate:api": "openapi-generator-cli generate"
  },
  "dependencies": {
    "@xstate/react": "^1.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.27.0",
    "styled-components": "^5.3.3",
    "xstate": "^4.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@mdx-js/react": "^1.6.22",
    "@openapitools/openapi-generator-cli": "^2.4.13",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@types/babel__core": "^7.1.16",
    "@types/mdx-js__react": "^1.5.4",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.4.1",
    "react-is": "^17.0.2",
    "typescript": "^4.4.4"
  },
  "browserslist": [
    "last 2 Firefox versions",
    "last 2 Chrome versions"
  ],
  "@parcel/bundler-default": {
    "http": 1
  }
}
